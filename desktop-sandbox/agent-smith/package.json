{
  "name": "agent-smith",
  "version": "0.1.0",
  "description": "A VoltAgent application",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env ./src",
    "dev:party": "wrangler dev src/partyserver.ts",
    "deploy:party": "wrangler deploy src/partyserver.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/agent-smith.cjs --target=node20",
    "start": "node dist/agent-smith.cjs",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "volt": "volt"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.88",
    "@ai-sdk/openai-compatible": "^1.0.29",
    "@voltagent/cli": "^0.1.10",
    "@voltagent/core": "^1.0.0",
    "@voltagent/logger": "^1.0.0",
    "@voltagent/server-hono": "^1.0.0",
    "@voltagent/serverless-hono": "^1.0.10",
    "ai": "^5.0.114",
    "dotenv": "^16.4.7",
    "partyserver": "^0.0.75",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "esbuild": "^0.27.2",
    "tsdown": "^0.15.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "wrangler": "^4.56.0"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
