{
  "name": "heyatlas",
  "version": "0.1.1",
  "description": "Tunnel local AI agents to the cloud for voice-powered interactions",
  "author": "Bishwendu Kundu <bishwenduk029@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/heyatlas",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/heyatlas.git",
    "directory": "cli"
  },
  "bin": {
    "heyatlas": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun run --hot index.ts",
    "build": "bun build index.ts --target=node --outfile=dist/cli.js && { echo '#!/usr/bin/env node'; cat dist/cli.js; } > dist/cli.tmp && mv dist/cli.tmp dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "@mcpc-tech/acp-ai-provider": "^0.2.3",
    "agents": "^0.2.35",
    "ai": "^6.0.7",
    "zod": "^4.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "cli",
    "voice",
    "agent",
    "tunnel",
    "heyatlas"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
